<%= form_for @article do |f| %>

  <%= f.label :title, "Entrez le titre" %>
  <%= f.text_field :title %>

  <%= f.label :description, "Entrez une description" %>
  <%= f.text_field :description %>

  <%= f.label :url, "Entrez l'url medium" %>
  <%= f.text_field :url %>

  <%= f.label :picture, "Entrez l'url de la photo" %>
  <%= f.text_field :picture %>

  <%= f.submit %>
<% end %>
