<%= form_for @video do |f| %>

  <%= f.label :title, "Entrez le titre" %>
  <%= f.text_field :title %>

  <%= f.label :description, "Entrez une description" %>
  <%= f.text_field :description %>

  <%= f.label :url, "Entrez l'url de la video" %>
  <%= f.text_field :url %>

  <%= f.label :picture, "Entrer l'url de la miniature" %>
  <%= f.text_field :picture %>

  <%= f.submit %>
<% end %>
