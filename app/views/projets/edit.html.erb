<h1>Une modification à apporter ?</h1>

<%= form_for @projet do |f| %>

  <%= f.label :title, "Entrer le titre" %>
  <%= f.text_field :title %>

  <%= f.label :description, "Entrer la description" %>
  <%= f.text_field :description %>

  <%= f.label :url, "Entrer l'url" %>
  <%= f.text_field :url %>

  <%= f.label :picture, "Entrer l'url d'une photo qui représente le projet" %>
  <%= f.text_field :picture %>

  <%= f.submit %>
<% end %>
